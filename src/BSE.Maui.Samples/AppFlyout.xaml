<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:BSE.Maui.Samples.Views"
            xmlns:views="clr-namespace:BSE.Maui.Samples.Views"
            x:Class="BSE.Maui.Samples.AppFlyout"
            Title="AppFlyout"  FlyoutLayoutBehavior="Default">
    <FlyoutPage.Flyout>
        <views:FlyoutMenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:MainPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>